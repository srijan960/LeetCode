class Solution {
    public void reverseString(char[] s) {
        int start = 0;
        int end = s.length - 1;
        while(start < end){
            char l = s[start];
            char r = s[end];
            if(l != r){
                s[start] = r;
                s[end] = l;
            } 
            start++;
            end--;
        }
    }
}
