class Solution {
    public int longestPalindromeSubseq(String s) {
        StringBuilder ss = new StringBuilder(s);
        String p = String.valueOf(ss.reverse());
        
        int m = s.length();
        int n = p.length();
        
        int[][] dp = new int[m + 1][n + 1];
        
        for(int i = 1; i <= m; i++){
            for(int j = 1; j <= n; j++){
                if(s.charAt(i - 1) == p.charAt(j - 1)){
                    dp[i][j] = 1 + dp[i - 1][j - 1];
                }
                else{
                    dp[i][j] = Math.max(dp[i - 1][j], dp[i][j - 1]);
                }
            }
        }
        
        return dp[m][n] ;
       
    }
}
