class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int i=m-1;
        int j=n-1;
        int ptr=nums1.length-1;
        while(ptr>=0){
            if(i<0){
                nums1[ptr]=nums2[j];
                j--;
            }
            else if(j<0){
                nums1[ptr]=nums1[i];
                i--;
            }
            else{
                if(nums1[i]>nums2[j]){
                nums1[ptr]=nums1[i];
                i--;
                }
                else{
                    nums1[ptr]=nums2[j];
                    j--;
                }
            }
            ptr--;}
    }
}
